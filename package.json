{
  "name": "postcss-sass",
  "version": "0.0.0",
  "description": "PostCSS plugin to parse PostCSS AST from SASS, using gonzales-pe.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-sass",
    "sass"
  ],
  "author": "Oleh Aloshkin <olegaleshkin@gmail.com>",
  "license": "MIT",
  "repository": "AleshaOleg/postcss-sass",
  "bugs": {
    "url": "https://github.com/AleshaOleg/postcss-sass/issues"
  },
  "homepage": "https://github.com/AleshaOleg/postcss-sass",
  "dependencies": {
    "gonzales-pe": "^4.0.3",
    "postcss": "^5.2.6"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^18.0.0",
    "lint-staged": "^3.3.1"
  },
  "scripts": {
    "test": "jest && eslint *.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "env": {
      "jest": true
    }
  },
  "pre-commit": ["test", "lint-staged"]
}
